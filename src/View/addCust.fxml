<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="486.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddCustController">
   <children>
      <TextField fx:id="custNameText" layoutX="142.0" layoutY="61.0" />
      <TextField fx:id="phoneText" layoutX="142.0" layoutY="130.0" />
      <TextField fx:id="addressText" layoutX="142.0" layoutY="179.0" />
      <TextField fx:id="postalText" layoutX="142.0" layoutY="252.0" />
      <Label layoutX="42.0" layoutY="65.0" text="Customer Name" />
      <Label layoutX="56.0" layoutY="183.0" text="Address" />
      <Label layoutX="46.0" layoutY="256.0" text="Postal Code" />
      <Label layoutX="37.0" layoutY="134.0" text="Phone Number" />
      <Button fx:id="saveButton" layoutX="129.0" layoutY="368.0" mnemonicParsing="false" onAction="#saveAddCust" text="Save" />
      <Button fx:id="cancelButton" layoutX="239.0" layoutY="368.0" mnemonicParsing="false" onAction="#toMain" text="Cancel" />
      <TableView fx:id="countryTable" layoutX="371.0" layoutY="61.0" prefHeight="337.0" prefWidth="221.0">
        <columns>
          <TableColumn fx:id="stateCol" prefWidth="219.0" text="State" />
        </columns>
      </TableView>
      <Label layoutX="64.0" layoutY="313.0" text="Country" />
      <ComboBox fx:id="countryCombo" layoutX="142.0" layoutY="309.0" onAction="#getCountry" prefWidth="150.0" />
   </children>
</AnchorPane>
